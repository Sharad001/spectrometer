[uwsgi]
socket = :9090
pythonpath = /opt/stack/stackalytics/dashboard/
module = web:app
env = STACKALYTICS_CONF=/usr/local/etc/stackalytics/stackalytics.conf
master = true
workers = 4
sharedarea = 4
harakiri = 30
daemonize = /var/log/uwsgi.log
pidfile = /var/run/uwsgi.pid
uid = stackalytics
gid = stackalytics
