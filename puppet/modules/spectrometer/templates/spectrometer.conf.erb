[DEFAULT]
# Run in debug mode?
# debug = True

# Default data - pull from GitHub by default
default_data_uri = <%= @_default_data_uri %>

# The folder that holds all project sources to analyze
sources_root = <%= @_sources_root %>

# Runtime storage URI
runtime_storage_uri = <%= @_runtime_storage_uri %>

# Hostname where dashboard listens on
listen_host = <%= @_listen_host %>

# Port where dashboard listens on
listen_port = <%= @_listen_port %>

# Number of days to update members
days_to_update_members = 7

# The address of file with corrections data
corrections_uri = <%= @_corrections_uri %>

# URI of review system
review_uri = <%= @_review_uri %>

# SSH key for gerrit review system access
ssh_key_filename = <%= @_ssh_key_filename %>

# SSH username for gerrit review system access
ssh_username = <%= @_ssh_username %>

# Forcibly read default data and update records
force_update = <%= @_force_update %>

# The address of file with list of programs
program_list_uri = <%= @_programs_uri %>

# Default metric
default_metric = <%= @_default_metric %>

# Default release, the most recent if not set
default_release = <%= @_default_release %>

# Default project type
default_project_type = <%= @_default_project_type %>

# The interval specifies how frequently dashboard should check for updates in seconds
dashboard_update_interval = <%= @_dashboard_update_interval %>
