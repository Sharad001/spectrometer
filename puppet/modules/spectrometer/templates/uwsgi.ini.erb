[uwsgi]
socket = :<%= @_uwsgi_port %>
pythonpath = <%= @_install_dir %>/dashboard/
module = web:app
env = STACKALYTICS_CONF=<%= @_config_dir %>/<%= @_config_file %>
master = true
workers = 4
sharedarea = 4
harakiri = 30
daemonize = <%= @_log_dir %>/<%= @_log_file %>
pidfile = <%= @_uwsgi_pid_file %>
uid = <%= @_user %>
gid = <%= @_group %>
