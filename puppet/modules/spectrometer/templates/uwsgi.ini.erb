[uwsgi]
socket = :<%= @uwsgi_port %>
pythonpath = <%= @install_dir %>/dashboard/
module = web:app
env = STACKALYTICS_CONF=<%= @config_dir %>/<%= @config_file %>
master = true
workers = 4
sharedarea = 4
harakiri = 30
daemonize = <%= @log_dir %>/<%= @log_file %>
pidfile = <%= @uwsgi_pid_file %>
uid = <%= @user %>
gid = <%= @group %>
